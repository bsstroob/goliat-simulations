{
    "study_name": "GOLIAT Far-Field Environmental Study",
    "user_email": "example@example.com",
    "phantoms": [
        "thelonius"
    ],
    "frequencies_mhz": [
        450
    ],
    "far_field_setup": {
        "type": "environmental",
        "environmental": {
            "incident_directions": [
                "x_pos"
            ],
            "polarizations": [
                "theta", "phi"
            ]
        },
        "auto_induced": {
            "grid_resolution_deg": 15
        }
    },
    "simulation_parameters": {
        "bbox_padding_m": 0,
        "global_gridding_resolution_m": 0.005,
        "simulation_bbox_points": 8,
        "global_auto_termination": "GlobalAutoTerminationUserDefined",
        "convergence_level_dB": -15,
        "simulation_time_multiplier": 5
    },
    "solver_settings": {
        "kernel": "Cuda",
        "location": "localhost",
        "show_solver_output": true
    },
    "verbose": true
}